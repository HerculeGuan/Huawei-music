ay: block;\n                width: 5px;\n                height: 5px;\n                border-radius: 50%;\n                background: $color2-darker;\n                margin: 0 4px;\n\n                &.current {\n                    width: 7px;\n                    height: 7px;\n                    background: $color2;\n                }\n            }\n        }\n    }\n\n    .container {\n        width: 200vw;\n        display: flex;\n        overflow: hidden;\n        transform: translateX(-0vw);\n        transition: transform 0.3s;\n        align-items: center;\n        height: calc(100vh - 265px);\n        &.panel1 {\n            transform: translateX(-0vw);\n        }\n        &.panel2 {\n            transform: translateX(-100vw);\n        }\n        .panel-effect {\n            width: 100vw;\n            height: 100%;\n            display: flex;\n            flex-direction: column;\n            justify-content: center;\n            .effect {\n                position: relative;\n                display: flex;\n                justify-content: center;\n                align-items: center;\n                height: 40vh;\n\n                >div {\n                    background-size: contain;\n                    position: absolute;\n                }\n\n                .effect-1 {\n                    background: url(../svg/effect-no-move.svg) 0 0 no-repeat;\n                    width: 70vw;\n                    height: 70vw;\n                }\n\n                .effect-2 {\n                    background: url(../svg/effect-move1.svg) 0 0 no-repeat;\n                    width: 60vw;\n                    height: 60vw;\n                    animation: rotate 20s linear infinite reverse;\n                }\n\n                .effect-3 {\n                    background: url(../svg/effect-move2.svg) 0 0 no-repeat;\n                    width: 24vw;\n                    height: 24vw;\n                    animation: rotate 15s linear infinite;\n                }\n            }\n\n            .lyrics {\n                text-align: center;\n                // margin-top: 10%;\n                p {\n                    font-size: 13px;\n                    color: $color1;\n                    margin-top: 8px;\n\n                    &.current {\n                        color: $color;\n                    }\n                }\n            }\n        }\n        .panel-lyrics {\n            width: 100vw;\n            height: 100%;\n            text-align: center;\n            line-height: 2;\n            overflow: hidden;\n            .lyrics-box {\n                transition: all 3s;\n                transform: translateY(-100px);\n            }\n            p {\n                font-size: 14px;\n                color: $color1;\n                &.current {\n                    color: $color;\n                }\n            }\n        }\n    }\n\n    .footer {\n\n        height: 155px;\n\n        .buttons {\n            display: flex;\n            justify-content: center;\n\n            svg {\n                width: 24px;\n                height: 24px;\n                margin: 0 20px;\n            }\n        }\n\n        .area-bar {\n            color: $color1;\n            font-size: 12px;\n            display: flex;\n            padding: 0 20px;\n            margin-top: 20px;\n            align-items: center;\n\n            .time-start {\n                order: 1;\n            }\n\n            .time-end {\n                order: 3;\n            }\n\n            .bar {\n                order: 2;\n                flex: 1;\n                height: 4px;\n                background: $color3;\n                border-radius: 2px;\n                margin: 0 10px;\n\n                .progress {\n                    width: 0%;\n                    height: 100%;\n                    border-radius: 2px;\n                    background: $color2;\n                    position: relative;\n\n                    &::after {\n                        content: '';\n                        position: absolute;\n                        right: 0;\n                        top: -7px;\n                        display: block;\n                        width: 16px;\n                        height: 16px;\n                        background: url(../svg/progress.svg) 0 0 no-repeat;\n                        background-size: contain;\n                    }\n                }\n            }\n        }\n\n        .actions {\n            display: flex;\n            align-items: center;\n            justify-content: space-between;\n            margin-top: 20px;\n            padding: 0 20px;\n\n            svg {\n                width: 28px;\n                height: 28px;\n\n                &.btn-pause {\n                    width: 50px;\n                    height: 50px;\n                }\n                &.no-circle {\n                    opacity: 0.5;\n                }\n                &.is-circle {\n                    opacity: 1;\n                    border: 1px solid orange;\n                    border-radius: 50%;\n                }\n            }\n        }\n    }\n\n}\n\n\n@keyframes rotate {\n    0% {\n        transform: rotate(0)\n    }\n\n    100% {\n        transform: rotate(360deg)\n    }\n}"},"lineCount":null}},"error":null,"hash":"0b437069faff9e512c0b522a025a6219","cacheData":{"env":{}}}